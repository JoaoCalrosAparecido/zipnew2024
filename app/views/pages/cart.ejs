<form action="create-preference" method="post">
    <section class="form-payment">
        <article class="products">
            <h2 class="title">Resumo do pedido</h2>
            
            <% var pedidos = pedidos || []; %>
            <%
            var total = 0;
            if (pedidos.length > 0) {
                pedidos.forEach(function(item) {
                    total += (item.qtde * item.preco);
            %>
                    <article class="item">
                        <span class="price hidden" id="summary-price">R$ <%= item.preco.toFixed(2) %></span>
                        <span class="price" id="subtot-price">R$ <%= (item.qtde * item.preco).toFixed(2) %></span>
                        <p class="item-name"> <%= item.produto %>
                        <span id="summary-quantity"> <%= item.qtde %></span> x R$ <%= item.preco.toFixed(2) %></span>
                        </p>
                    </article>
            <%
                });
            } else {
            %>
                <p>Carrinho vazio.</p>
            <%
            }
            %>
            
            <article class="total">
                <span class="price" id="summary-total">Total R$ <%= total.toFixed(2) %></span>
            </article>
        </article>
    </section>

